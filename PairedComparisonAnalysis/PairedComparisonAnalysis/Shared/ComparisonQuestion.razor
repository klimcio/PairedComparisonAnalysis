<div>
    <button @onclick="ModalInstance.CloseAsync" class="btn btn-primary">@Comparison.First</button>
    <h1> > </h1>
    <button @onclick="ModalInstance.CancelAsync" class="btn btn-secondary">@Comparison.Second</button>
</div>

@code {
    [CascadingParameter] BlazoredModalInstance ModalInstance { get; set; }

    [Parameter]
    public Comparison Comparison { get; set; }
}